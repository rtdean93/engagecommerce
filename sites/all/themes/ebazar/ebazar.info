name = EBazar - Premium Retina Responsive HTML5-CSS3 Bootstrap 3 Drupal Commerce Theme 1.0
description = <strong>IMPORTANT NOTE</strong>: This theme is part of a Drupal Installation Profile and should not be used independently over an existing Drupal 7.x installation.<br /> <br />For theme FAQs, info & "How to..." visit <a href="http://demos.doublemthemes.com/ebazar/documentation/"><strong>EBazar Documentation</strong></a> or contact us via <a href="http://support.doublemthemes.com"><strong>our support forums</strong></a>. 
core = 7.x
base theme = bootstrap


;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[navigation]     = 'Navigation'
